<greatbone>
    <web>
        <host name="op" bind="10.0.0.202:8080"/>
        <host name="mgt" bind="10.0.0.202:8081"/>
    </web>
    <grid bind="10.0.0.202" cluster="10.0.0.201-202">
        <dataset name="users">
            <part id="jx" cap="4096"/>
        </dataset>
        <dataset name="shops" parts="jx" load="date > (current_date() - 3)"/>
        <dataset name="individuals" parts="jx"/>
        <dataset name="demands" parts="jxnc, jxgz" load="date > (current_date() - 3)"/>
        <dataset name="orders" parts="jx" load="date > (current_date() - 3)"/>
        <dataset name="journals" parts="jx" capacity="1024"/>
        <dataset name="transacts" parts="jx"/>
        <fileset name="clips" parts="jx"/>
    </grid>
    <db>
        <source user="postgres" password="postgres" database="postgres"/>
        <source key="dir" url="jdbc:postgresql://localhost" user="postgres" password="postgres" database="postgres"/>
    </db>
</greatbone>